<!DOCTYPE html>
<html>
    <html lang="en">

<head>
  <meta charset="utf-8">
  <meta content="width=device-width, initial-scale=1.0" name="viewport">

  <title>Kakotel Group - S.A</title>
  <meta content="" name="description">
  <meta content="" name="keywords">

  <!-- Favicons -->
  <link href="assets/img/favicon.jpeg" rel="icon">
  <link href="assets/img/apple-touch-icon.jpeg" rel="apple-touch-icon">

  <!-- Google Fonts -->
  <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Raleway:300,300i,400,400i,600,600i,700,700i,900" rel="stylesheet">

  <!-- Vendor CSS Files -->
  <link href="assets/vendor/animate.css/animate.min.css" rel="stylesheet">
  <link href="assets/vendor/aos/aos.css" rel="stylesheet">
  <link href="assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
  <link href="assets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
  <link href="assets/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
  <link href="assets/vendor/glightbox/css/glightbox.min.css" rel="stylesheet">
  <link href="assets/vendor/swiper/swiper-bundle.min.css" rel="stylesheet">

  <!-- Template Main CSS File -->
  <link href="assets/css/style.css" rel="stylesheet">

 
</head>

<body>

 {% if header %}
        
    {% for head in header %}
        
  <!-- ======= Top Bar ======= -->
  <section id="topbar" class="d-flex align-items-center">
    <div class="container d-flex justify-content-center justify-content-md-between">
      <div class="contact-info d-flex d-hide align-items-center">
         {% if head.email %}
                <i class="bi bi-envelope-fill"></i><a href="mailto:contact@example.com">{{head.email}} </a>
        {% endif %}
          {% if head.email %}
                 <i class="bi bi-phone-fill phone-icon"></i> {{head.telephone}}
        {% endif %}
       
      
      </div>
      <div class="social-links d-md-block">
        <a href="#" class="francais"><img src="assets/img/francais-flag.png" width="24px" title="Francais"></a>
      <a href="#" class="angais"><img src="assets/img/anglais-flag.png" width="24px" title="English"></a>
      <a href="#" class="espagnol"><img src="assets/img/espagnol-flag.png" width="24px" title="espagnol"></a>
      </div>
    </div>
  </section>
    
   
  <!-- ======= Header ======= -->
  <header id="header" class="d-flex align-items-center">
    <div class="container d-flex align-items-center">

      <div class="logo me-auto">
        {% if head.logo %}
               <h1><a href="index.html">{{head.logo}} </a></h1>
        {% endif %}
     
        <!-- Uncomment below if you prefer to use an image logo -->
        <!-- <a href="index.html"><img src="assets/img/logo.png" alt="" class="img-fluid"></a>-->
      </div>

      <nav id="navbar" class="navbar">

         
        <ul>
          {% for menu in head.menus %}
             {% if menu %}
                
            {% if menu.sousmenus.count > 0 %}
                
          
        
          <li class="dropdown"><a class="nav-link active" href= {{menu.link}}><span>{{menu.nomfr}} </span> <i class="bi bi-chevron-down"></i></a>
            <ul>
               {% for sousmenus in menu.sousmenus %}
                 <li><a href={{sousmenus.link}}>{{sousmenus}} </a></li>
               {% endfor %}
            </ul>
          </li>
          {% else %}
          <li><a class="nav-link" href={{menu.link}}>{{menu.nomfr}} </a></li>
         
            {% endif %}
            {% endif %}
            {% endfor %}
        </ul>
        <i class="bi bi-list mobile-nav-toggle"></i>
      </nav><!-- .navbar -->

    </div>
  </header><!-- End Header -->
   {% endfor %}
 {% endif %}

 


        {% block body %}{% endblock %}


   
  
  <!-- ======= Footer ======= -->

   {% if footer %}
    
  
  <footer id="footer">
    {% for foot in footer %}
        
    
    <div class="container">
      <div class="copyright">
        &copy; Copyright <strong><span>{{foot.logo}} </span></strong>.{{foot.phrase}}
      </div>
      
    </div>
     {% endfor %}

   {% endif %}  
  </footer><!-- End Footer -->

  <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

  <!-- Vendor JS Files -->
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.0/jquery.min.js"></script>
  <script src="assets/vendor/purecounter/purecounter.js"></script>
  <script src="assets/vendor/aos/aos.js"></script>
  <script src="assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
  <script src="assets/vendor/glightbox/js/glightbox.min.js"></script>
  <script src="assets/vendor/isotope-layout/isotope.pkgd.min.js"></script>
  <script src="assets/vendor/swiper/swiper-bundle.min.js"></script>
  <script src="assets/vendor/php-email-form/validate.js"></script>
  
  <!-- Template Main JS File -->
  <script src="assets/js/custom-selectbox.js"></script>
  <script src="assets/js/main.js"></script>

</body>

</html>     
    </body>
</html>
